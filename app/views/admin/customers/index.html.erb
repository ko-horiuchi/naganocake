<h1>顧客一覧画面</h1>
  <table>
    <thead>
      <tr>
        <th></th>
        <th>姓</th>
        <th>名</th>
        <th colspan="3"></th>
      </tr>
    </thead>
      s
    <tbody>
      <% @customers.each do |customer| %>
        <tr>
          <td>
            <%= link_to admin_customer_path(customer.id) do %>
            <%= customer.last_name %>
            <% end %>
          </td>
          <td>
            <%= link_to admin_customer_path(customer.id) do %>
            <%= customer.first_name %>
            <% end %>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
